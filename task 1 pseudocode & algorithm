
////  no 1
STORE 'MASSA' with 600
STORE 'PERCEPATAN' with 2
CALCULATE 'MASSA' times 'PERCEPATAN'
SET 'RESULTAN GAYA' with CALCULATION RESULTAN



//////// no 2
STORE 'TAHUN' with any value
IF 'TAHUN' % 4 = 0 AND 'TAHUN' % 100 != 0
    DISPLAY 'KABISAT'
ELSE IF 'TAHUN' % 4 = 0 AND 'TAHUN' % 100 =0 AND 'TAHUN' % 400 = 0
    DISPLAY 'KABISAT'
ELSE 
    DISPLAY 'BUKAN KABISAT'



//// no 3
SET 'PAKAIAN' = 1
WHILE 'PAKAIAN' <= 20
    DO 'MASUKAN PAKAIAN'

DO 'NYALAKAN MESIN CUCI'

// no 4
SET 'SISWA' = 1
WHILE 'SISWI' <= 40
    IF (KUKU SISWA = 'PANJANG')
        DO 'HUKUM SISWA'
    ELSE
        DO 'PUJI SISWA'
